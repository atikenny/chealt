{
  "name": "@chealt/ui",
  "version": "0.0.0",
  "description": "Chealt UI",
  "main": "scr/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.config.js",
    "start": "yarn watch",
    "test": "jest --config=config/jest/unit.config.js",
    "test:ui": "concurrently --success first --kill-others \"BUILD_ENV=ci yarn start\" \"yarn test:ui:wait && yarn test:ui:run\"",
    "test:ui:local": "yarn test:ui:run",
    "test:ui:debug": "DEBUG=true yarn test:ui",
    "test:ui:run": "JEST_PUPPETEER_CONFIG=config/jest/ui.puppeteer.config.js jest --config=config/jest/ui.config.js",
    "test:ui:wait": "wait-on -t 60000 http-get://localhost:9000/",
    "watch": "webpack-dev-server --config config/webpack.config.js"
  },
  "author": "Attila Bartha",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.1",
    "dotenv": "^6.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "jest-puppeteer": "^3.3.1",
    "puppeteer": "^1.11.0",
    "redux-devtools-extension": "^2.13.5",
    "style-loader": "^0.23.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
