{
  "name": "Chealt",
  "version": "0.0.1",
  "description": "An ML powered app for staying healthy and balanced.",
  "main": "src/index.js",
  "docs": "documents",
  "scripts": {
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "eslint . --ext .js,.jsx",
    "postbuild": "yarn test:coverage && yarn coveralls",
    "precommit": "yarn verify",
    "server": "BABEL_ENV=server nodemon src/index.js --config config/nodemon.json --exec babel-node",
    "start": "yarn server",
    "test": "jest --config=config/jest/jest.config.js",
    "test:coverage": "yarn test --coverage",
    "verify": "yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atikenny/Chealt.git"
  },
  "keywords": [
    "Chealt",
    "health"
  ],
  "author": "atikenny",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atikenny/Chealt/issues"
  },
  "homepage": "https://github.com/atikenny/Chealt#readme",
  "dependencies": {
    "deeplearn": "^0.5.1",
    "express": "^4.16.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.15.0-rc.8",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "nodemon": "^1.17.2"
  }
}
